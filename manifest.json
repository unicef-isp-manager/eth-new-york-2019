{
  "name": "No Cookies",
  "description": "Removes 'Cookie' and 'Set-Cookie' headers.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "unicef-logo.png",
    "default_popup": "popup.html"
   }
  
}
