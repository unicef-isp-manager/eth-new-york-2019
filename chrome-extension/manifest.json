{
  "name": "Blocksolid",
  "description": "Monitors connectivity",
  "version": "2.0",
  "manifest_version": 2,
  "permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": { 
    "16": "icons/blue-16px.png",
    "48": "icons/blue-48px.png",
    "128": "icons/blue-128px.png"
  },
  "browser_action": {
    "default_icon": "icons/blue-19px.png"
   }
  
}
